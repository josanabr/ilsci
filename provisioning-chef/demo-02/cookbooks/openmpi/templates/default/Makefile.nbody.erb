SOURCES = ${nbodypipe_SOURCES}

all-redirect: ${PROGRAMS}

clean: 
	rm -f *.o ${PROGRAMS} *~ PI*

.SUFFIXES: .o .c .f .cc .f90 
.c: 
	${CC} ${CFLAGS} -o $* $< ${LIBS} 
.c.o: 
	${CC} ${CFLAGS} -c $< 
.f: 
	${F77} ${FFLAGS} -o $* $< ${LIBS} 
.f.o: 
	${F77} ${FFLAGS} -c $< 
.f90: 
	${F90} ${F90FLAGS} -o $* $< ${LIBS} 
.cc: 
	${CXX} ${CXXFLAGS} -o $* $< ${LIBS}

run: ${PROGRAMS} 
	${MPIRUN} ${NP_ARG} 2 --hostfile machinefile <%= @shareddir %>/nbodypipe 1000

sources: 
	@echo ${SOURCES}


